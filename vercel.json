{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "pages/app/api/s3-compress-and-upload/route.js": {
      "maxDuration": 60
    },
    "pages/app/api/resolve-code/route.js": {
      "maxDuration": 30
    }
  }
} 